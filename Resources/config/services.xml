<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="f_devs_static_page.model.static_page.class">FDevs\StaticPageBundle\Model\StaticPage</parameter>
        <parameter key="f_devs_static_page.form.type.page_type.class">FDevs\StaticPageBundle\Form\Type\StaticTypeType</parameter>
    </parameters>

    <services>
        <service id="f_devs_static_page.form.type.type" class="%f_devs_static_page.form.type.page_type.class%">
            <tag name="form.type" alias="fdevs_static_type"/>
            <call method="setChoices"><argument>%f_devs_static_page.pages%</argument></call>
        </service>
    </services>

</container>
